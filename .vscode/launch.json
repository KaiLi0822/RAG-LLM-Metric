{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debugpy: Python: Attach 5678",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "justMyCode": true
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "preLaunchTask": "Activate Conda Environment",
      "env": { "PYTHONPATH": "${workspaceRoot}"}
    }
  ]
}
